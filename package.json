{
  "name": "cotfea",
  "displayName": "cotfea",
  "description": "cotfea",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "syntaxes": "js-yaml ./syntaxes/cotfea.tmLanguage.yml > ./syntaxes/cotfea.tmLanguage.json"
  },
  "main": "./src/extension.js",
  "activationEvents": [
    "onLanguage:semanticLanguage"
  ],
  "contributes": {
    "languages": [
      {
        "id": "cotfea",
        "extensions": [
          ".cotfea"
        ]
      },
      {
        "id": "semanticLanguage",
        "extensions": [
          ".semanticLanguage"
        ]
      }
    ],
    "grammars": [
      {
        "language": "cotfea",
        "scopeName": "source.cotfea",
        "path": "./syntaxes/cotfea.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@swc/cli": "^0.1.34",
    "@swc/core": "^1.2.47",
    "js-yaml": "^3.14.1"
  }
}
