{
  "name": "cotfea",
  "displayName": "cotfea",
  "description": "cotfea",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "syntaxes": "js-yaml ./syntaxes/cotfea.tmLanguage.yml > ./syntaxes/cotfea.tmLanguage.json"
  },
  "main": "./DefaultColor/Semantic/dist/extension.dist.js",
  "activationEvents": [
    "onLanguage:semanticLanguage"
  ],
  "contributes": {
    "languages": [
      {
        "id": "cotfea",
        "extensions": [
          ".cotfea"
        ]
      },
      {
        "id": "semanticLanguage",
        "extensions": [
          ".semanticLanguage"
        ]
      }
    ],
    "grammars": [
      {
        "language": "cotfea",
        "scopeName": "source.cotfea",
        "path": "./syntaxes/cotfea.tmLanguage.json",
        "path_comment": "./DefaultColor/RegexColor/cotfea.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "GapStyle VS",
        "uiTheme": "vs-dark",
        "path": "./themes/Sorcisto-color-theme.json"
      }
    ]
  },
  "devDependencies": {
    "@swc/cli": "^0.1.39",
    "@swc/core": "^1.2.54",
    "js-yaml": "^4.1.0"
  }
}